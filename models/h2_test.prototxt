## crop
layer {
  name: "crop2_1" type: "Crop" bottom: "w2_1down_upsample" bottom: "w1_1top" top: "w2_1down_crop"
}
layer {
  name: "crop2_2" type: "Crop" bottom: "w2_2down_upsample" bottom: "w1_1top" top: "w2_2down_crop"
}
