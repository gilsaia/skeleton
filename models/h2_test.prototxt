  top:"softmax_h1s3_4"
}
layer {
  type: "Softmax" 
  name: "softmax_h1s4_5"
  bottom: "crop_h1s4_5"
  top:"softmax_h1s4_5"
}

## slice
layer{
  type: "Slice" name: "slice_h1s1_2"
  bottom: "crop_h1s1_2"
  top: "slice_h1s1_2_0"
  top: "slice_h1s1_2_1"
  slice_param {
    axis: 1
    slice_point: 1
  }
}
layer{
  type: "Slice" name: "slice_h1s2_3"
  bottom: "crop_h1s2_3"
  top: "slice_h1s2_3_0"
  top: "slice_h1s2_3_1"
  top: "slice_h1s2_3_2"
  slice_param {
    axis: 1
    slice_point: 1
    slice_point: 2
  }
}
layer{
  type: "Slice" name: "slice_h1s3_4"
  bottom: "crop_h1s3_4"
  top: "slice_h1s3_4_0"
  top: "slice_h1s3_4_1"
  top: "slice_h1s3_4_2"
  top: "slice_h1s3_4_3"
  slice_param {
    axis: 1
    slice_point: 1
    slice_point: 2
    slice_point: 3
  }
}
layer{
  type: "Slice" name: "slice_h1s4_5"
  bottom: "crop_h1s4_5"
  top: "slice_h1s4_5_0"
  top: "slice_h1s4_5_1"
  top: "slice_h1s4_5_2"
  top: "slice_h1s4_5_3"
  top: "slice_h1s4_5_4"
  slice_param {
    axis: 1
    slice_point: 1
    slice_point: 2
    slice_point: 3
    slice_point: 4
  }
}
## concat
layer {
  name: "h1_cat0" type: "Concat" 
  bottom: "slice_h1s1_2_0" bottom: "slice_h1s2_3_0" bottom: "slice_h1s3_4_0" bottom: "slice_h1s4_5_0"
  top: "h1_cat0" 
  concat_param { concat_dim: 1} 
}
layer {
  name: "h1_cat1" type: "Concat" 
  bottom: "slice_h1s1_2_1" bottom: "slice_h1s2_3_1" bottom: "slice_h1s3_4_1" bottom: "slice_h1s4_5_1"
  top: "h1_cat1" 
  concat_param { concat_dim: 1} 
}
layer {
